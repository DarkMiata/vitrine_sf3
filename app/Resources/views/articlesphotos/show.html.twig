{% extends 'admin.html.twig' %}

{% block body %}
    <h1>Articlesphoto</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ articlesPhoto.id }}</td>
            </tr>
            <tr>
                <th>Refarticle</th>
                <td>{{ articlesPhoto.refArticle }}</td>
            </tr>
            <tr>
                <th>Filename</th>
                <td>{{ articlesPhoto.fileName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('articlesphotos_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('articlesphotos_edit', { 'id': articlesPhoto.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
