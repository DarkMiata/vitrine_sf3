{% extends 'admin.html.twig' %}

{% block body %}
    <h1>Articlescategory</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ articlesCategory.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ articlesCategory.name }}</td>
            </tr>
            <tr>
                <th>Countarticles</th>
                <td>{{ articlesCategory.countArticles }}</td>
            </tr>
            <tr>
                <th>Cattypeid</th>
                <td>{{ articlesCategory.catTypeId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('articlescategories_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('articlescategories_edit', { 'id': articlesCategory.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
